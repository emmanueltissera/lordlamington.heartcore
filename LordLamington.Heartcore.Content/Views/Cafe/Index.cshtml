@model Cafe

<div class="ck_bc_main_wrapper float_left"  id="page-shop">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="col-6">
                    <h3>A little bit of indulgence at our cafe in</h3>
                    <h1><a href="#">@Model.Title</a></h1>
                </div>
                <div class="col-6">
                    <div class="x_contact_title_icon_cont_main_box">
                        <div class="x_contact_title_icon"> <i class="fas fa-map-marker-alt"></i>
                        </div>
                        <div class="x_contact_title_icon_cont">
                            <p>@Model.BuildingNumber @Model.AddressLine1 @Model.AddressLine2</p>
                            <p>@Model.Suburb @Model.State @Model.Postcode</p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
